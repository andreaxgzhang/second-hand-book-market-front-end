
/* form style */
.user-input {
  width: 300px;
  margin-bottom: 35rpx;
}
.input-height {
  height: 10em;
}
.input-label {
  font-size: 35rpx;
}
textarea {
  padding: 10rpx 0;
  font-size: 35rpx;
  width: 100%;
}
.text-area-wrap {
  padding: 15rpx 15rpx;
  background-color: #fff;
  margin-top:8px;
  border-radius: 2px;
  border-left: 5px solid #9F9F9C;
}
